{
  "name": "@chaptercheck/shared",
  "version": "0.0.0",
  "private": true,
  "exports": {
    "./validations/*": "./src/validations/*.ts",
    "./utils": "./src/utils.ts",
    "./audioFileNames": "./src/audioFileNames.ts",
    "./hooks/*": "./src/hooks/*.ts",
    "./types/*": "./src/types/*.ts"
  },
  "typesVersions": {
    "*": {
      "validations/*": [
        "./src/validations/*.ts"
      ],
      "utils": [
        "./src/utils.ts"
      ],
      "audioFileNames": [
        "./src/audioFileNames.ts"
      ],
      "hooks/*": [
        "./src/hooks/*.ts"
      ],
      "types/*": [
        "./src/types/*.ts"
      ]
    }
  },
  "dependencies": {
    "@chaptercheck/convex-backend": "workspace:*",
    "zod": "^4.3.6"
  },
  "peerDependencies": {
    "convex": ">=1.31.0",
    "react": ">=18.0.0"
  }
}
